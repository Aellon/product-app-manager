<link href="{{url "/themes/appm/css/es_statistics.css"}}" rel="stylesheet" media="screen"
      xmlns="http://www.w3.org/1999/html">


<input type="hidden" id="page" value="4">
<div class="content-div">
    <div class="row-fluid">
        <div class="row">
            <div class="col-md-2" >
                {{>left-nav}}
            </div>
            <div class="col-md-10 asset-view-container">
                <div>
                    <div class="pull-left" style="margin-top:50px;" align="left"><h2>Web Application Statistics</h2></div>
                    <div id="rangeSliderWrapper" class="well" >
                        <div class="row-fluid">
                            <div id="dateRangePickerContainer" >
                                <div class="btn-group" role="group" aria-label="...">
                                    <button id="hour-btn" type="button" class="btn btn-default">Hour</button>
                                    <button id="today-btn"type="button" class="btn btn-default">Today</button>
                                    <button id="week-btn" type="button" class="btn btn-default">Week</button>
                                    <div class="input-append" style="display:inline" >
                                        <input type="text" id="date-range" style="width: 270px;display:inline;" name="dateRange" class="form-control" value=""  />
                                        <span class="add-on-cal"><i style="margin-right:0px;" class="icon-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>

                <div class="clearfix"></div>
                <div id="graphContainer">
                    <div id="slidercontainer" style="width:100%;height:100%; ">
                        <div id="pie-chart"></div>
                    </div>
                    <div class="clearfix"></div>
                    <div id="pagination" ></div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="backdrop"></div>