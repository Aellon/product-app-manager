<input type="hidden" id="page" value="1">
<div class="content-div">
    <div class="row-fluid">
        <div class="row">
            <div class="pull-left heading" align="left"><h1>Web Application Statistics - Usage by App</h1></div>
        </div>
        <div class="row">
            <div class="col-md-2 left-nav" >
                {{>left-nav}}
            </div>
            <div class="col-md-10 asset-view-container">

                <div >
                    <div id="rangeSliderWrapper" class="well" >
                        <div class="row-fluid">
                            <div id="dateRangePickerContainer" >
                                <div class="btn-group" role="group">
                                    <button id="hour-btn" type="button" class="btn btn-default date-range">Hour</button>
                                    <button id="today-btn" type="button" class="btn btn-default date-range">Today</button>
                                    <button id="week-btn" type="button" class="btn btn-default date-range">Week</button>
                                    <button id="month-btn" type="button" class="btn btn-default date-range active">Month</button>
                                    <button id="date-range" type="button" class="btn btn-default date-range last-child"
                                            data-toggle="popup" title="Click to set custom date range">
                                </div>
                                <span class="add-on-cal"><i style="margin-right:0px;" class="icon-calendar"></i></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>

                <div class="widget">
                    <div class="widget-content">
                        <div class="graph-container center-wrapper widget-table1 pull-left">
                            <div id="placeholder3" class="graph-placeholder"></div>
                        </div>
                        <div class="graph-container widget-table" id="userUsage">
                            <div id="placeholder31" class="graph-placeholder"></div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div id="chartContainer">
                    <div id="tableContainer" class="center-wrapper" style="max-width:80%;"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
    <div class="backdrop"></div>