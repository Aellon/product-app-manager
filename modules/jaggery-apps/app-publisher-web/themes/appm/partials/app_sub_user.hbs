<input type="hidden" id="page" value="5">
<div class="content-div">
    <div class="row-fluid">
        <div class="row">
            <div class="pull-left" align="left"><h2>Web Application Statistics</h2></div>
        </div>
        <div class="row">
            <div class="col-md-2" >
                {{>left-nav}}
            </div>
            <div class="col-md-10 asset-view-container">

                <div >
                    <div id="rangeSliderWrapper" class="well" >
                        <div class="row-fluid">
                            <div id="dateRangePickerContainer" >
                                <div class="btn-group" role="group" aria-label="...">
                                    <button id="hour-btn" type="button" class="btn btn-default">Hour</button>
                                    <button id="today-btn"type="button" class="btn btn-default">Today</button>
                                    <button id="week-btn" type="button" class="btn btn-default">Week</button>
                                    <div class="input-append" style="display:inline" >
                                        <input type="text" id="date-range" style="width: 270px;display:inline;" name="dateRange" class="form-control" value=""  />
                                        <span class="add-on-cal"><i style="margin-right:0px;" class="icon-calendar"></i></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>

                <div class="widget">
                    <div class="widget-head">
                        <div class="pull-left">{{t "Apps subscribed by User "}}</div>
                        <div class="widget-controls">
                            <a href="#" class="btn-remove"><i class="icon icon-remove"></i></a>
                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <div class="widget-content">
                        <div class="container1">
                        <div class="form-group form-inline" id="searchUserForm" >
                            <label>Subscriber</label>
                            <input type="text" class="form-control" style="width: 200px; " id="search" />
                            <button id="target" class="btn btn-default" >Search</button>
                       </div>
                            <div id="placeholder2" class="graph-placeholder"></div>
                        </div>
                        <div class="graph-container widget-graph2">
                            <div id="flot-placeholder" class="graph-placeholder"></div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
</div>
<div class="backdrop"></div>